{
    "name": "backendtea/phpstan-multiple-error-formatter",
    "keywords": ["dev"],
    "type": "phpstan-extension",
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "^1.12||^2.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BackEndTea\\ErrorFormatter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gert de Pagter",
            "email": "backendtea@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config.neon"
            ]
        }
    }
}
